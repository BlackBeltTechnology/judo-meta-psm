@cached
operation JUDOPSM!DataProperty displayName() : String {
	return self.getEntityType().name + "." + self.name;
}

@cached
operation JUDOPSM!DataProperty getEntityType() : JUDOPSM!EntityType {
	return JUDOPSM!EntityType.all.selectOne(et | et.dataProperties.includes(self));
}

@cached
operation JUDOPSM!DataProperty isPrimitive() : Boolean {
	return self.dataType.isKindOf(JUDOPSM!Primitive);
}
