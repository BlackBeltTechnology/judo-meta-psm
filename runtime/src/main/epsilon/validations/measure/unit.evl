import "../../operations/measure/_importMeasure.eol";

context JUDOPSM!Unit {
    
    // unit name should be unique
    critique UnitNameUsUnique {
        check: not JUDOPSM!Unit.all.excluding(self).selectOne(u | u.name == self.name or u.symbol == self.name or self.symbol.isDefined() and (u.name = self.symbol or u.symbol.isDefined() and u.symbol = self.symbol)).isDefined()
        message: "Unit name/symbol is not unique: " + self.name + "/" + self.symbol
    }
}
