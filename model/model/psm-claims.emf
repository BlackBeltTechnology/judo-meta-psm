@namespace(uri="http://blackbelt.hu/judo/meta/psm/claims", prefix="psmClaims")
package psmClaims;

class ClaimMappings {
  val ClaimMapping[*] claimMappings;
}

class ClaimMapping {
  attr String[1] claimName;
  attr String[1] realm;
  attr String[1] claimType;
  attr Integer size;
  attr Integer precision;
  attr Integer scale;
}
