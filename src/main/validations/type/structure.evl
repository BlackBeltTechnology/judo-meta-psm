import "../../operations/type/_importType.eol";

context JUDOPSM!Structure {

    // recursive structures are not supported, entity types should be used instead
	constraint RecursiveStructureIsNotSupported {
		check: not self.hasCircularReference()
		message: "RecursiveStructureIsNotSupported|Element " + self.name + " is a recursive structure that is not supported"
	}
}