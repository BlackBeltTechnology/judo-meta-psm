context JUDOPSM!String {

    // maxLength > 0
	constraint InvalidMaxLength {
		check: self.maxLength > 0
		message: "InvalidMaxLength|MaxLength must be greater than 0: " + self.name
	}

    // maxLength <= 4000
	constraint MaxLengthIsTooLarge {
		check: self.maxLength <= 4000
		message: "MaxLengthIsTooLarge|MaxLength must be less than/equals to 4000: " + self.name
	}
}