import "../../operations/type/_importType.eol";

context JUDOPSM!Type {

    // type names should be unique
    critique TypeNamesAreUnique {
        check: not JUDOPSM!Type.all.excluding(self).selectOne(t | t.name == self.name and t.type() <> self.type()).isDefined()
        message: "Type name is not unique: " + self.name
    }
}
